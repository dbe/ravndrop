//CSS for h3h3 landing page "layout blank, and page.template.blank"
#vape-naysh-container h1,
#vape-naysh-container h2,
#vape-naysh-container h3,
#vape-naysh-container h4,
#vape-naysh-container h5,
#vape-naysh-container h6 {
  font-weight: 100;
  color: white;
}

#vape-naysh-container * {
  font-family: "Heiti TC", "Helvetica neue";
  text-align: center;
}

#naysh-logo {
  width: 150px;
}

#ethan-box {
  padding: 40px;
}

#vape-naysh-pic {
  margin: auto;
}


#join-row {
  margin-bottom: 50px;
  background-color: #F05D5E;
  height: 300px;
}


#buy-row {
  padding-top: 30px;
  padding-bottom: 50px;
  text-align: center;
  background-color: #2DD881;
}

#faq-row {
  background-color: #0F7173;
  min-height: 300px;
  padding-bottom: 100px;
}

#faq-row li {
  padding: 10px;
  list-style-type: none;
  text-align: left;
}

question::before {
  content: "+";
}

question {
  text-align: left !important;
  color: white;
  font-size: 26px;
  display: block;
  cursor: pointer;
}

answer {
  margin-left: 40px;
  text-align: left !important;
  color: white;
  color: white;
  font-size: 22px;
  display: none;
}


.btn {
  background: #FFCF56;
  color: #0F7173;
}

#buy-one-button {
  margin-right: 40px;
  border-radius: 0;
}

#buy-many-button {
  border-radius: 0;
}

#fupa-message {
  color: white;
}

#how-many-div {
  display: none;
  color: white;
  padding: 30px;
}

#how-many-label {
  margin-right: 10px;
}

#how-many-input {
  width: 50px;
  color: black;
}
